{
  "claudekit_commands": {
    "setup_installation": [
      {
        "button_text": "Interactive Setup",
        "command": "claudekit setup",
        "tooltip": "Interactive setup wizard",
        "requires_input": false
      },
      {
        "button_text": "Quick Setup",
        "command": "claudekit setup --yes",
        "tooltip": "Quick setup with defaults",
        "requires_input": false
      },
      {
        "button_text": "Install All Agents",
        "command": "claudekit setup --all",
        "tooltip": "Install everything (all agents)",
        "requires_input": false
      },
      {
        "button_text": "Install to User Directory",
        "command": "claudekit setup --user",
        "tooltip": "Install in ~/.claude instead of project",
        "requires_input": false
      }
    ],
    "list_show": [
      {
        "button_text": "List All Components",
        "command": "claudekit list",
        "tooltip": "Show all components (hooks, commands, agents, config)",
        "requires_input": false
      },
      {
        "button_text": "List Agents",
        "command": "claudekit list agents",
        "tooltip": "List available agents with token counts",
        "requires_input": false
      },
      {
        "button_text": "List Commands",
        "command": "claudekit list commands",
        "tooltip": "List available commands with token counts",
        "requires_input": false
      },
      {
        "button_text": "Show Agent",
        "command": "claudekit show agent {agent_id}",
        "tooltip": "Display agent prompt for external use",
        "requires_input": true,
        "input_prompt": "Enter agent ID:",
        "input_label": "Agent ID",
        "has_json_format": true
      },
      {
        "button_text": "Show Command",
        "command": "claudekit show command {command_id}",
        "tooltip": "Display command prompt for external use",
        "requires_input": true,
        "input_prompt": "Enter command ID:",
        "input_label": "Command ID",
        "has_json_format": true
      }
    ],
    "diagnostics": [
      {
        "button_text": "Doctor Check",
        "command": "claudekit doctor",
        "tooltip": "Check installation and validate documentation links",
        "requires_input": false
      },
      {
        "button_text": "STM Status",
        "command": "claudekit status stm",
        "tooltip": "Check Simple Task Master installation status",
        "requires_input": false
      }
    ],
    "linting": [
      {
        "button_text": "Lint Commands",
        "command": "claudekit lint-commands",
        "tooltip": "Lint slash command markdown files",
        "requires_input": false
      },
      {
        "button_text": "Lint Agents",
        "command": "claudekit lint-agents",
        "tooltip": "Lint agent markdown files",
        "requires_input": false
      }
    ],
    "hooks": [
      {
        "button_text": "Run Hook",
        "command": "claudekit-hooks run {hook_name}",
        "tooltip": "Execute a specific hook manually",
        "requires_input": true,
        "input_prompt": "Enter hook name to run:",
        "input_label": "Hook Name"
      },
      {
        "button_text": "Profile All Hooks",
        "command": "claudekit-hooks profile",
        "tooltip": "Profile all configured hooks performance",
        "requires_input": false
      },
      {
        "button_text": "Profile Hook",
        "command": "claudekit-hooks profile {hook_name} --iterations {iterations}",
        "tooltip": "Profile specific hook with iterations",
        "requires_input": true,
        "input_prompt": "Enter hook name to profile:",
        "input_label": "Hook Name",
        "requires_number": true,
        "number_prompt": "Number of iterations:",
        "number_label": "Iterations",
        "number_default": 5,
        "number_min": 1,
        "number_max": 100
      },
      {
        "button_text": "Disable Hook",
        "command": "claudekit-hooks disable {hook_name}",
        "tooltip": "Disable hook for current session",
        "requires_input": true,
        "input_prompt": "Enter hook name to disable:",
        "input_label": "Hook Name"
      },
      {
        "button_text": "Enable Hook",
        "command": "claudekit-hooks enable {hook_name}",
        "tooltip": "Re-enable hook for current session",
        "requires_input": true,
        "input_prompt": "Enter hook name to enable:",
        "input_label": "Hook Name"
      },
      {
        "button_text": "Hook Status",
        "command": "claudekit-hooks status {hook_name}",
        "tooltip": "Show hook status (enabled/disabled)",
        "requires_input": true,
        "input_prompt": "Enter hook name to check status:",
        "input_label": "Hook Name"
      }
    ]
  },
